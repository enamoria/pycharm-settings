<keymap version="1" name="Default copy" parent="$default">
  <action id="InvalidateCaches">
    <keyboard-shortcut first-keystroke="shift ctrl alt r" />
  </action>
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="ctrl alt q" />
  </action>
  <action id="PrevSplitter">
    <keyboard-shortcut first-keystroke="ctrl alt w" />
  </action>
  <action id="ShowIntentionActions">
    <keyboard-shortcut first-keystroke="alt enter" />
    <keyboard-shortcut first-keystroke="altGraph enter" />
  </action>
</keymap>